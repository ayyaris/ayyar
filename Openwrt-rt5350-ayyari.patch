Index: target/linux/ramips/files/arch/mips/ralink/rt305x/mach-ayyar.c
===================================================================
--- target/linux/ramips/files/arch/mips/ralink/rt305x/mach-ayyar.c	(revision 0)
+++ target/linux/ramips/files/arch/mips/ralink/rt305x/mach-ayyar.c	(working copy)
@@ -0,0 +1,108 @@
+/*
+ *   AYYAR Real time web control board support by Ayyari Abdennaby <ayyari@me.com>
+ *  
+ * 
+ *
+ *  This program is free software; you can redistribute it and/or modify it
+ *  under the terms of the GNU General Public License version 2 as published
+ *  by the Free Software Foundation.
+ */
+
+#include <linux/init.h>
+#include <linux/platform_device.h>
+#include <linux/spi/spi.h>
+#include <linux/spi/flash.h>
+#include <linux/gpio.h>
+
+#include <asm/mach-ralink/machine.h>
+#include <asm/mach-ralink/dev-gpio-buttons.h>
+#include <asm/mach-ralink/dev-gpio-leds.h>
+#include <asm/mach-ralink/rt305x.h>
+#include <asm/mach-ralink/rt305x_regs.h>
+
+#include "devices.h"
+
+#define AYYAR_GPIO_BUTTON_RESET	0
+#define AYYAR_GPIO_LED_WPS		20
+#define AYYAR_GPIO_LED_SYSTEM		19
+
+#define AYYAR_KEYS_POLL_INTERVAL	20
+#define AYYAR_KEYS_DEBOUNCE_INTERVAL	(3 * AYYAR_KEYS_POLL_INTERVAL)
+
+const struct flash_platform_data ayyar_flash = {
+        .type           = "mx25l6405d",
+};
+
+struct spi_board_info ayyar_spi_slave_info[] __initdata = {
+        {
+                .modalias       = "m25p80",
+                .platform_data  = &ayyar_flash,
+                .irq            = -1,
+                .max_speed_hz   = 10000000,
+                .bus_num        = 0,
+                .chip_select    = 0,
+        }
+};
+
+static struct gpio_led ayyar_leds_gpio[] __initdata = {
+        {
+                .name           = "ayyar:green:system",
+                .gpio           = AYYAR_GPIO_LED_SYSTEM,
+                .active_low     = 1,
+        },
+        {
+                .name           = "ayyar:green:power",
+                .gpio           = AYYAR_GPIO_LED_WPS,
+                .active_low     = 1,
+                .default_state  = LEDS_GPIO_DEFSTATE_ON,
+        }
+};
+
+static struct gpio_keys_button ayyar_gpio_buttons[] __initdata = {
+        {
+                .desc           = "reset",
+                .type           = EV_KEY,
+                .code           = KEY_RESTART,
+                .debounce_interval = AYYAR_KEYS_DEBOUNCE_INTERVAL,
+                .gpio           = AYYAR_GPIO_BUTTON_RESET,
+                .active_low     = 1,
+        }
+};
+static struct gpio ayyar_gpios[] = {
+	{ 1, GPIOF_IN, "GPIO 1" },
+	{ 2, GPIOF_IN, "GPIO 2" },
+	{ 7, GPIOF_OUT_INIT_LOW, "GPIO 7" },
+	{ 8, GPIOF_OUT_INIT_LOW, "GPIO 8" },
+	{ 9, GPIOF_OUT_INIT_LOW, "GPIO 9" },
+	{ 10, GPIOF_OUT_INIT_LOW, "GPIO 10" },
+	{ 11, GPIOF_OUT_INIT_LOW, "GPIO 11" },
+	{ 12, GPIOF_OUT_INIT_LOW, "GPIO 12" },
+	{ 13, GPIOF_OUT_INIT_LOW, "GPIO 13" },
+	{ 14, GPIOF_OUT_INIT_LOW, "GPIO 14" },
+};
+
+static void __init ayyar_init(void)
+{
+
+	rt305x_gpio_init((RT305X_GPIO_MODE_GPIO <<
+					RT305X_GPIO_MODE_UART0_SHIFT) |
+					RT305X_GPIO_MODE_I2C |
+					RT305X_GPIO_MODE_MDIO |
+					RT305X_GPIO_MODE_JTAG);
+        
+ 	gpio_request_array(ayyar_gpios, ARRAY_SIZE(ayyar_gpios));
+	gpio_free_array(ayyar_gpios, ARRAY_SIZE(ayyar_gpios));                                
+
+	rt305x_register_spi(ayyar_spi_slave_info, ARRAY_SIZE(ayyar_spi_slave_info));
+	ramips_register_gpio_leds(-1, ARRAY_SIZE(ayyar_leds_gpio),ayyar_leds_gpio);                    
+	ramips_register_gpio_buttons(-1, AYYAR_KEYS_POLL_INTERVAL, ARRAY_SIZE(ayyar_gpio_buttons), ayyar_gpio_buttons); 
+
+        rt305x_esw_data.vlan_config = RT305X_ESW_VLAN_CONFIG_LLLLW;
+        rt305x_register_ethernet();
+        rt305x_register_wifi();
+        rt305x_register_wdt();
+        rt305x_register_usb();
+}
+
+MIPS_MACHINE(RAMIPS_MACH_AYYAR, "AYYAR", "AYYAR",
+             ayyar_init);
